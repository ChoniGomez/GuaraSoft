<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="GuaraSoftPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>logicaDeNegocio.Articulo</class>
    <class>logicaDeNegocio.Cliente</class>
    <class>logicaDeNegocio.ComprobanteComercial</class>
    <class>logicaDeNegocio.CuentaCorriente</class>
    <class>logicaDeNegocio.Cuota</class>
    <class>logicaDeNegocio.Detalle</class>
    <class>logicaDeNegocio.Empleado</class>
    <class>logicaDeNegocio.Empresa</class>
    <class>logicaDeNegocio.EnvioTercero</class>
    <class>logicaDeNegocio.Equipo</class>
    <class>logicaDeNegocio.Estado</class>
    <class>logicaDeNegocio.FacturaCompra</class>
    <class>logicaDeNegocio.FacturaVenta</class>
    <class>logicaDeNegocio.OrdenDeTrabajo</class>
    <class>logicaDeNegocio.Persona</class>
    <class>logicaDeNegocio.Proveedor</class>
    <class>logicaDeNegocio.Proyecto</class>
    <class>logicaDeNegocio.Stock</class>
    <class>logicaDeNegocio.Sucursal</class>
    <class>logicaDeNegocio.Tecnologia</class>
    <class>logicaDeNegocio.Tique</class>
    <class>logicaDeNegocio.Turno</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/GuaraSoft"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="jonathan"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
